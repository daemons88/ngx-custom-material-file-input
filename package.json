{
  "name": "library-workspace",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-library": "ng build material-file-input",
    "pack-lib": "cd dist/material-file-input && npm pack",
    "watch": "ng build --watch --configuration development",
